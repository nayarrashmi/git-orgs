{
  "name": "git-orgs",
  "version": "1.0.0",
  "description": "A tool to display organization info from github",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV='dev' webpack-dev-server --open",
    "build": "NODE_ENV='production' webpack -p",
    "create": "webpack",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "deploy": "npm run build  && firebase deploy",
    "firebase-init": "firebase login && firebase init",
    "validate": "npm-run-all --parallel test build",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ]
  },
  "dependencies": {
    "axios": "^0.21.3",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.6.2",
    "query-string": "^6.1.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-router-dom": "^4.3.1",
    "uglifyjs-webpack-plugin": "^2.1.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-dynamic-import-node": "^2.0.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "codecov": "^3.2.0",
    "coveralls": "^3.0.3",
    "css-loader": "^1.0.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-15": "^1.3.1",
    "enzyme-adapter-react-16": "^1.11.2",
    "firebase-tools": "^4.0.3",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "react-styleguidist": "^9.0.1",
    "react-test-renderer": "^16.8.4",
    "style-loader": "^0.21.0",
    "webpack": "^4.16.4",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  },
  "author": "rashmi nair <nair.rashmi1406@gmail.com>",
  "license": "ISC"
}
